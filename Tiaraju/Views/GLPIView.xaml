<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Tiaraju.Views.GLPIView"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="GLPI">
    <VerticalStackLayout>
        <WebView
            x:Name="navegador"
            HeightRequest="600"
            Navigated="paginaCarregada"
            Navigating="carregandoPagina"
            VerticalOptions="FillAndExpand" />
        <Label x:Name="lblStatus" />
    </VerticalStackLayout>
</ContentPage>